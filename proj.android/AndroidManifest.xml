<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.playscape.coco2dxdemo.iaptest">

    <!--
        ###################################################################################
        #                                ACTION  ITEMS                                    #
        # =============================================================================== #
        #                                                                                 #
        # 1. Replace-ALL com.playscape.coco2dxdemo strings with your game's package name. #
        # 2. PlayscapeActivity.java can either extend from your own activity or replace   #
        #   it entirely, either way is good.                                              #
        # 3. Please note that the application tag has an android:name tag which points to #
        #   the PlayscapeApp class, should you choose not to use it, but to use your own  #
        #   class, simply call PlayscapeStartup.getInstance().onCreate(this); in that     #
        #   class's onCreate() method.                                                    #
        #                                                                                 #
        ###################################################################################
    -->

    <uses-sdk android:minSdkVersion="18"/>

    <uses-feature android:glEsVersion="0x00020000"/>

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    
    <uses-permission android:name="com.android.vending.BILLING"/>

    <!-- library -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <!-- GCM connects to Google Services. -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <!-- GCM requires a Google account. -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>

    <!-- Keeps the processor from sleeping when a message is received. -->
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <!--
	  Creates a custom permission so only this app can receive its messages.
	 
	  NOTE: the permission *must* be called PACKAGE.permission.C2D_MESSAGE,
	        where PACKAGE is the application's package name.
    -->
    <permission android:name="com.playscape.coco2dxdemo.permission.C2D_MESSAGE" android:protectionLevel="signature"/>

    <uses-permission android:name="com.playscape.coco2dxdemo.permission.C2D_MESSAGE"/>

    <!-- This app has permission to register and receive data message. -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"/>

    <supports-screens android:anyDensity="true" android:largeScreens="true" android:normalScreens="true" android:smallScreens="true"/>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="com.soomla.SoomlaApp">
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>

        <activity android:configChanges="orientation" android:label="@string/app_name" android:launchMode="singleTop" android:name="org.cocos2dx.simplegame.SimpleGame" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!-- GCM -->


        <!--
		  BroadcastReceiver that will receive intents from GCM
		  services and handle them to the custom IntentService.
		 
		  The com.google.android.c2dm.permission.SEND permission is necessary
		  so only GCM services can send data messages for the app.
        -->
        <receiver android:name="com.playscape.utils.gcm.GcmBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>

                <!-- Receives the actual messages. -->
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>

                <category android:name="com.playscape.coco2dxdemo"/>
            </intent-filter>
        </receiver>

        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"/>

        <!--
		  Application-specific subclass of GcmIntentService that will
		  handle received messages.
        -->
        <service android:name="com.playscape.utils.gcm.GcmIntentService"/>
        <!--
		  BroadcastReceiver that will receive intents from NotificationManager and
		  handle received messages.
        -->
        <receiver android:name="com.playscape.utils.gcm.handlers.NotificationActionListener">
            <intent-filter>
                <action android:name="com.playscape.utils.intent.NOTIFICATIONACTION"/>

                <category android:name="com.playscape.coco2dxdemo"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.playscape.utils.gcm.handlers.AutomaticRetryExponentialBackOff">
            <intent-filter>
                <action android:name="com.playscape.utils.intent.RETRY_BACKOFF"/>

                <category android:name="com.playscape.coco2dxdemo"/>
            </intent-filter>
        </receiver>
        <!-- Remote Logger -->

        <service android:name="mominis.common.logger.LoggingService">
            <intent-filter>
                <action android:name="com.mominis.gameconsole.logger.START_LOGGER_SERVICE"/>
            </intent-filter>
        </service>
        <service android:name="com.mominis.logger.IsAliveService"/>

        <receiver android:name="mominis.common.logger.OnLogAlarmReceiver">
            <intent-filter>
                <action android:name="com.mominis.gameconsole.alarm.START_LOG_SHIPPING_FROM_REPEATING"/>
                <action android:name="com.mominis.gameconsole.alarm.START_LOG_SHIPPING_FROM_DEFERRED"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.mominis.logger.ConnectivityChangedReceiver">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>
            </intent-filter>
        </receiver>
        <!-- Used for install referrer tracking -->
        <receiver android:exported="true" android:name="com.playscape.analytics.MarketReferrerReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
        
        <!-- Google billing -->
        <activity android:name="com.soomla.store.billing.google.GooglePlayIabService$IabActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen"/>
        <meta-data android:name="billing.service" android:value="google.GooglePlayIabService"/>


        <!-- StartApp Ads -->
        <activity android:name="com.startapp.android.publish.list3d.List3DActivity" android:theme="@android:style/Theme"/>
        <activity android:configChanges="orientation|keyboardHidden" android:name="com.startapp.android.publish.AppWallActivity" android:theme="@android:style/Theme.Translucent"/>
        <!-- MoPub Ads -->
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.mopub.mobileads.MoPubActivity"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.mopub.mobileads.MraidActivity"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.mopub.mobileads.MraidBrowser"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.mopub.mobileads.MraidVideoPlayerActivity"/>
        <!-- Vungle Video Ads -->
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.vungle.sdk.VungleAdvert" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>

        <service android:name="com.vungle.sdk.VungleIntentService"/>
        <!-- AdColony Video Ads -->
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.jirbo.adcolony.AdColonyOverlay" android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.jirbo.adcolony.AdColonyFullscreen" android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.jirbo.adcolony.AdColonyBrowser" android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"/>
        <!-- AdMob Ads -->
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" android:name="com.google.android.gms.ads.AdActivity"/>
        <!-- Millenial Media Ads -->
        <activity android:configChanges="keyboardHidden|orientation|keyboard" android:name="com.millennialmedia.android.VideoPlayer"/>
        <activity android:configChanges="keyboardHidden|orientation|keyboard" android:name="com.millennialmedia.android.MMActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
			
			
			<!-- playscape SDK -->
		  <!-- Activities -->


        <activity android:configChanges="keyboard|keyboardHidden" android:exported="true" android:name="mominis.gameconsole.activities.GamePageActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="mominis.common.PlayscapeSdk.SHOW_GAME_PAGE_ACTION"/>
            </intent-filter>
        </activity>
        
        <activity android:configChanges="keyboard|keyboardHidden" android:exported="true" android:name="mominis.gameconsole.activities.RedirectToMarketActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar">
        </activity>
        
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenSize" android:exported="true" android:label="@string/app_name" android:launchMode="singleTask" android:name="mominis.gameconsole.views.impl.Catalog" android:theme="@android:style/Theme.NoTitleBar">
        </activity>
        
        <activity android:name="mominis.gameconsole.activities.GameDialogActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar">       
        </activity>
        		
		<!-- Notification with promotionUrl trigger this activity -->
        <activity android:label="Mominis Promotion" android:name="mominis.common.services.notifications.PromotionActivity" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        </activity>
		
        <!-- Notification with market trigger this activity, it is used merely for analytics and launching the market afterwards -->
        <activity android:label="Notification" android:name="mominis.common.services.notifications.NotificationProxyActivity" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        </activity>

        <!-- Our Services -->
        
        <service android:name="mominis.common.services.sync.impl.SyncIntentService">
			<intent-filter>
				<action android:name="android.content.Intent.ACTION_SYNC"/>
				<action android:name="mominis.common.services.sync.impl.SyncIntentService.SCHEDULE_PERIODIC_SYNC_ACTION"/>
			</intent-filter>
		</service>  	
		
		<service android:name="mominis.common.components.mailslot.impl.MailslotIntentService">
			<intent-filter>
				<action android:name="mominis.common.components.mailslot.impl.MailslotIntentService.MAILSLOT_ACTION"/>
			</intent-filter>
		</service>     	
        
		<service android:enabled="true" android:exported="true" android:name="mominis.gameconsole.services.impl.SocialService"/>    	
				
		
        <!-- 3rd Party Services -->
        <service android:name="mobpartner.ad.sdk.MobPartnerSDK$ProcessCode"/>

        
		<!-- Our Broadcast Receivers -->
		
		
		<!-- Used to trigger GameApplication.onCreate() to update the master in case of app removal. -->
		<receiver android:enabled="true" android:exported="true" android:name="mominis.common.services.notifications.PackageStateChangeReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <action android:name="android.intent.action.PACKAGE_REPLACED"/>
				<action android:name="android.intent.action.PACKAGE_REMOVED"/>
				<data android:path="*mominis*"/>
                <data android:scheme="package"/>
            </intent-filter>
        </receiver>
        
		
	  	<!-- Initializes the notifications subsystem -->
		<receiver android:enabled="true" android:exported="true" android:name="mominis.common.services.notifications.GameNotificationInitReceiver">
			<intent-filter>
				<action android:name="com.mominis.GameNotificationInitReceiver.ACTION"/>
			</intent-filter>
	  	</receiver>
        
        <!-- Responds to notifications related alarms -->
        <receiver android:enabled="true" android:exported="true" android:name="mominis.common.services.notifications.OnGameNotificationAlarmReceiver">
            <intent-filter>
                <action android:name="com.mominis.game.notifications.OnGameNotificationAlarmReceiver.ACTION"/>
            </intent-filter>
        </receiver>
	  
		<!--  Package changes receiver -->
	    <receiver android:name="mominis.gameconsole.common.PackageStateReceiverObservable">
			<intent-filter>
                 <action android:name="android.intent.action.PACKAGE_REMOVED"/>
                 <action android:name="android.intent.action.PACKAGE_REPLACED"/>
                 <action android:name="android.intent.action.PACKAGE_ADDED"/>
                 <data android:path="*mominis*"/> 
                 <data android:scheme="package"/>
			</intent-filter>
		</receiver>
		
	    	
    </application>

</manifest>
